{
  "name": "octolinker",
  "version": 2,
  "builds": [
    {
      "src": "api/legacy/handler.js",
      "use": "@now/node"
    },
    {
      "src": "api/handler.js",
      "use": "@now/node"
    },
    {
      "src": "website/package.json",
      "use": "@now/next"
    }
  ],
  "routes": [
    {
      "src": "/api/?",
      "dest": "api/handler.js"
    },
    {
      "src": "/api/.*",
      "dest": "api/legacy/handler.js"
    },
    {
      "src": "/(.*)",
      "dest": "/website/$1",
      "headers": {
        "x-request-path": "$1"
      }
    }
  ],
  "regions": [
    "bru1",
    "gru1",
    "hnd1",
    "iad1",
    "sfo1"
  ],
  "alias": [
    "octolinker.now.sh"
  ],
  "env": {
    "MIXPANEL_TOKEN": "@mixpanel_token",
    "REDIS_PASSWORD_BRU1": "@redis_password_bru1",
    "REDIS_PASSWORD_GRU1": "@redis_password_gru1",
    "REDIS_PASSWORD_HND1": "@redis_password_hnd1",
    "REDIS_PASSWORD_IAD1": "@redis_password_iad1",
    "REDIS_PASSWORD_SFO1": "@redis_password_sfo1"
  }
}
