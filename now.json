{
  "name": "octolinker-api",
  "version": 2,
  "builds": [
    { "src": "src/handler.js", "use": "@now/node" },
    { "src": "index.html", "use": "@now/static" }
  ],
  "routes": [
    { "src": "/?", "dest": "src/handler.js" }
  ],
  "regions": ["bru1", "gru1", "hnd1", "iad1", "sfo1"],
  "alias": ["octolinker-api.now.sh"],
  "env": {
    "MIXPANEL_TOKEN": "@mixpanel_token",
    "DEFAULT_REDIS_PORT": "10363",
    "DEFAULT_REDIS_PASSWORD": "@default_redis_password",
    "DEFAULT_REDIS_HOST": "@default_redis_url",
    "BRU1_REDIS_PORT": "19483",
    "BRU1_REDIS_PASSWORD": "@bru1_redis_password",
    "BRU1_REDIS_HOST": "@bru1_redis_url"
  }
}
