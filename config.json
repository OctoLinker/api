{
  "bower": {
    "registry": "https://registry.bower.io/packages/%s",
    "fallback": "https://bower.io/search/?q=%s",
    "xpaths": [
      "/url"
    ]
  },
  "composer": {
    "registry": "https://packagist.org/packages/%s.json",
    "fallback": "https://packagist.org/packages/%s",
    "xpaths": [
      "/package/repository"
    ]
  },
  "rubygems": {
    "registry": "https://rubygems.org/api/v1/gems/%s.json",
    "fallback": "https://rubygems.org/search?query=%s",
    "xpaths": [
      "/source_code_uri",
      "/homepage_uri",
      "/project_uri"
    ]
  },
  "npm": {
    "registry": "https://registry.npmjs.org/%s",
    "fallback": "https://www.npmjs.com/package/%s",
    "xpaths": [
      "/url",
      "/repository",
      "/repository/url",
      "/repository/path",
      "/repository/web",
      "/repository/git",
      "/repositories",
      "/repositories/url",
      "/repositories/path",
      "/repositories/web",
      "/repositories/git",
      "/homepage"
    ]
  },
  "pypi": {
    "registry": "https://pypi.python.org/pypi/%s/json",
    "fallback": "https://pypi.python.org/pypi/%s",
    "xpaths": [
      "/info/home_page",
      "/info/package_url"
    ]
  },
  "crates": {
    "registry": "https://crates.io/api/v1/crates/%s",
    "fallback": "https://crates.io/crates/%s",
    "xpaths": [
      "/crate/repository",
      "/crate/homepage"
    ]
  }
}
